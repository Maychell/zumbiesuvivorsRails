<h1>Survivors</h1>
<%= link_to 'Back', root_path %>
<%= link_to 'New Survivor', new_survivor_path %>
<table>
  <thead>
    <th>id</th>
    <th>Name</th>
    <th>Gender</th>
    <th>Age</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th></th>
    <th>infected?</th>
  </thead>
  <% @survivors.each do |survivor| %>
    <tbody>
      <td><%= survivor.id %></td>
      <td><%= survivor.name %></td>
      <td><%= survivor.gender %></td>
      <td><%= survivor.age %></td>
      <td><%= survivor.latitude %></td>
      <td><%= survivor.longitude %></td>
      <td><%= link_to 'edit', edit_survivor_path(survivor) %></td>
      <td><%= survivor.infected %></td>
    </tbody>
  <% end %>
</table>